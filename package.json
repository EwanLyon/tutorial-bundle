{
  "name": "tutorial-bundle",
  "version": "0.0.0",
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "extension"
  ],
  "keywords": [
    "",
    "nodecg-bundle"
  ],
  "scripts": {
    "start": "nodecg start"
  },
  "nodecg": {
    "compatibleRange": "^1.9.0",
    "dashboardPanels": [
      {
        "name": "score",
